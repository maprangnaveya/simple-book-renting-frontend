{
  "name": "simple-renting-book-frontend",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": false
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "@rescript-mui/material",
    "@glennsl/rescript-json-combinators"
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-open JsonCombinators"
  ],
  "jsx": {
    "mode": "automatic",
    "version": 4
  }
}